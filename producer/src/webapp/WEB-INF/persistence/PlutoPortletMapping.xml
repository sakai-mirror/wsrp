<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN"
                         "http://castor.exolab.org/mapping.dtd">

<mapping>


        <class name="org.apache.wsrp4j.producer.provider.driver.ConsumerConfiguredPortletImpl">
                     <map-to xml="ConsumerConfiguredPortlet"/>

                <field name="ParentHandle"
                       type="java.lang.String"
                       get-method="getParentHandle"
                       set-method="setParentHandle">
                          <bind-xml name="ParentHandle" node="element"/>
                </field>

                <field name="PortletHandle"
                       type="java.lang.String"
                       get-method="getPortletHandle"
                       set-method="setPortletHandle">
                          <bind-xml name="PortletHandle" node="element"/>
                </field>

        </class>

</mapping>

